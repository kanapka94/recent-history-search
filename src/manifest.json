{
  "manifest_version": 3,
  "name": "Recent history search",
  "version": "0.0.3",
  "description": "Chrome extension for searching recently closed and history tabs with navigation using arrow keys and date filters.",
  "permissions": ["tabs", "history", "favicon", "storage"],
  "default_locale": "en",
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "16": "./icons/icon16.png",
      "48": "./icons/icon48.png",
      "128": "./icons/icon128.png"
    }
  },
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "options_ui": {
    "page": "./options/options.html"
  }
}
